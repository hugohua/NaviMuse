{
  "name": "navimuse",
  "version": "1.0.0",
  "description": "AI-driven music curator for Navidrome",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node --project tsconfig.server.json src/server.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node --project tsconfig.server.json src/server.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc -b client/tsconfig.json && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint ."
  },
  "keywords": [
    "navidrome",
    "ai",
    "music",
    "curator"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/compression": "^1.8.1",
    "@types/node-fetch": "^2.6.13",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.6.0",
    "bullmq": "^5.66.5",
    "compression": "^1.8.1",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "framer-motion": "^12.25.0",
    "helmet": "^8.1.0",
    "howler": "^2.2.4",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.9.1",
    "json5": "^2.2.3",
    "lucide-react": "^0.562.0",
    "md5": "^2.3.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.0.0",
    "qs": "^6.14.1",
    "sqlite-vec": "^0.1.7-alpha.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.0",
    "@types/howler": "^2.2.12",
    "@types/md5": "^2.3.0",
    "@types/node": "^20.0.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
