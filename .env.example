# Navidrome Configuration (Required)
ND_URL=http://localhost:4533
ND_USER=your_username
ND_PASS=your_password_or_token

# AI Configuration (Required)
# Compatible with OpenAI SDK (e.g., Aliyun Qwen, DeepSeek, OpenAI)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxx

# AI Configuration (Optional)
# Default: Aliyun Dashscope (Qwen)
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# Default: qwen3-max
# Default: qwen3-max
OPENAI_MODEL=qwen3-max
# Optional: Comma separated list of models to randomly choose from
# If not provided, defaults to [OPENAI_MODEL]
OPENAI_MODEL_LIST=deepseek-v3.2,qwen3-max

# Server Configuration (Optional)
PORT=3000
